{
  "BlobUpload": {
    "ContainerName": "logs",
    "BlobPrefix": "ingest/",
    "ContentType": "text/plain",
    "Overwrite": false,
    "MaxFileSizeBytes": 104857600,
    "MinFileSizeBytes": 1,
    "MaxRetryAttempts": 3,
    "RetryDelayMilliseconds": 1000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Azure": "Warning"
    }
  }
}
